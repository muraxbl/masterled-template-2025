/* VARIABLES DE COLOR */
* {
  -webkit-overflow-scrolling: touch;
}
:root {

	--color-principal: #333333;
	--azul-principal: #00338e;
	--enlace-principal: #003aa1;
	--enlace-hover: #bad405;
}
html {width:100%;max-width:100%;overflow-x:hidden;}
body {-webkit-box-sizing: border-box;-moz-box-sizing: border-box;box-sizing: border-box;position:relative;overflow-x:hidden;transition: margin 0.4s;-webkit-transition: margin 0.4s;max-width:100%;width:100%;transition-timing-function: cubic-bezier(.42,.27,0,.99);-webkit-transition-timing-function: cubic-bezier(.42,.27,0,.99)}
.bodyleft {margin:0 0 0 300px;transition: margin 0.5s;-webkit-transition:margin 0.5s;transition-timing-function: cubic-bezier(.42,.27,0,.99);-webkit-transition-timing-function: cubic-bezier(.42,.27,0,.99)}
@media (max-width:767px) {
	nav.header-nav {transition: margin 0.4s;-webkit-transition: margin 0.4s;-moz-transition: margin 0.4s}
	.bodyleft nav.header-nav {margin-left:300px;}
}
/* SHOP INFOS CABECERA (templates/_partials/shop-infos.tpl) */
.shop-infos-wrap {display:flex;width:100%;min-height:30px;background-color:#00338e;
flex-wrap: wrap;justify-content: space-around;align-items:center;
}
.shop-infos-button {display:none;}
@media (max-width:767px) {
	.shop-infos-wrap {justify-content:center;align-content:center;align-items:center;flex-direction:row;flex:0 0 50%;padding:20px 0;z-index:9999;display:block;position:fixed;bottom:0;left:0;width:100%; height:auto;transform: translateY(100%);-webkit-transform: translateY(100%);transition:all ease-in-out 0.3s;-webkit-transition:all ease-in-out 0.3s;-moz-transition:all ease-in-out 0.3s}
	.shop-infos-button {position:fixed;bottom:0;left:5px;width:2em;height:2em;display:block;z-index:99999;}
	.shop-infos-wrap div {width:50%;float:none;margin-left:auto;margin-right:auto;margin-top:5px;text-align: 	center}
	.shop-infos-button::after {content: "\e88e";
			display: inline !important;
		position: relative;
		right:0;bottom:0;
		vertical-align: middle;
		font-family:'Material Icons';
		font-size:2em;
		color:#e94c2f;
		height:0;
		line-height: 0;
		opacity:1;
		text-align: left;}
	.shop-infos-button:hover + .shop-infos-wrap, .shop-infos-button:active + .shop-infos-wrap, .shop-infos-button:focus + .shop-infos-wrap  {transform: translateY(0);-webkit-transform: translateY(0);}
}
.infofixed {position:fixed;margin-top:65px;box-shadow: 0px 2px 8px 0 rgba(0,0,0,.2);z-index:9999;}
.shop-infos-wrap span {color:#ffffff;font-weight:300;font-size:0.75em;}
.shop-infos-wrap span.pre-info {font-weight:bold;color:#f4f4f4;}
.shop-infos-wrap i {font-size:1.5em;vertical-align: middle;align-self:center;}
#contact-link a {color:#FFF;}
.superuser-front-container {z-index:1 !important}
@media (min-width:768px) and (max-width:992px) {
	.user-info > .logout ~ .account span {display:inline-block;width:8rem;overflow:hidden;text-overflow: ellipsis;}
}
@media (min-width:992px) {
.logo {width:200px;max-width:none !important;}
}
@media (max-width:575px) {
	.top-logo#_mobile_logo h1, #_mobile_logo {text-align: center;}
}
